    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^3.0.0",
    "adonis-preset-ts": "^2.0.0",
    "eslint": "^7.22.0",
    "eslint-plugin-adonis": "^1.2.1",
    "git-commit-msg-linter": "^3.0.3",
    "husky": "4",
    "lint-staged": "^10.5.4",
    "pino-pretty": "^4.7.1",
    "typescript": "~4.1",
    "youch": "^2.1.1",
    "youch-terminal": "^1.0.1"
  },
  "engines": {
    "node": "14.x"
  },
  "husky": {
    "hooks": {
