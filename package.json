{
  "name": "satheler-survey",
  "version": "1.0.0",
  "main": "build/server.js",
  "author": "Gustavo Bittencourt Satheler <gustavosatheler@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/satheler/satheler-survey",
    "type": "git"
  },
  "private": false,
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "serverless:start": "yarn build && sls offline",
    "deploy": "rimraf .serverless && sls deploy",
    "deploy:aws": "rimraf temp/layers && yarn layer:node_modules && yarn deploy -c serverless.aws.yml",
    "deploy:azure": "yarn deploy -c serverless.azure.yml",
    "layer:node_modules": "yarn install --modules-folder ./tmp/layers/nodejs/node_modules --production=true"
  },
  "dependencies": {
    "@adonisjs/auth": "^5.1.1",
    "@adonisjs/core": "^5.0.4-preview-rc",
    "@adonisjs/lucid": "^10.0.0",
    "@adonisjs/repl": "^1.0.0",
    "@satheler/s12r": "^0.2.1",
    "luxon": "^1.26.0",
    "pg": "^8.5.1",
    "phc-bcrypt": "^1.0.5",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "adonis-preset-ts": "^2.0.0",
    "eslint": "^7.22.0",
    "eslint-plugin-adonis": "^1.2.1",
    "git-commit-msg-linter": "^3.0.3",
    "husky": "4",
    "lint-staged": "^10.5.4",
    "pino-pretty": "^4.7.1",
    "rimraf": "^3.0.2",
    "serverless": "^2.30.3",
    "serverless-azure-functions": "^2.1.0",
    "serverless-offline": "^6.8.0",
    "typescript": "~4.1",
    "youch": "^2.1.1",
    "youch-terminal": "^1.0.1"
  },
  "engines": {
    "node": ">=12.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint"
    ]
  }
}
