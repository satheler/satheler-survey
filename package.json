{
  "name": "satheler-survey",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Gustavo Bittencourt Satheler <gustavosatheler@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/satheler/satheler-survey",
    "type": "git"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.production.json",
    "dev": "nodemon start/server.ts",
    "start": "node dist/start/server.js",
    "docker:up": "yarn build && docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "yarn build && docker build -t satheler/satheler-survey .",
    "docker:run": "docker run -p 8000:8000 satheler/satheler-survey",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "yarn test --watch -c jest.unit.config.js",
    "test:integration": "yarn test --watch -c jest.integration.config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/dynamodb": "^1.2.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "4",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "4",
    "jest": "^26.6.3",
    "jest-dynalite": "^3.3.1",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "aws-sdk": "^2.857.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "validator": "^13.5.2"
  }
}
